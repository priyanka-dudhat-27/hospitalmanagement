<script>
    <%if(locals.flash.success.length>0 && locals.flash.success){%>
        new Noty({
            type: 'success',
            layout: 'topRight',
            theme:'metroui',
            text: '<%=locals.flash.success%>',
            animation: {
                 open: 'animated bounceInRight', 
                 close: 'animated bounceOutRight',
            },
            duration:60
        }).show();
    <%}%>
    <%if(locals.flash.error.length>0 && locals.flash.error){%>
        new Noty({
            type: 'error',
            layout: 'topRight',
            theme:'metroui',
            text: '<%=locals.flash.error%>',
            animation: {
                 open: 'animated bounceInRight', 
                 close: 'animated bounceOutRight',
            },
            duration:600
        }).show();
    <%}%>
    
</script>

